  "bugs": {
    "url": "https://github.com/brando5393/project-3/issues"
  },
  "homepage": "https://github.com/brando5393/project-3#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "mongoose": "^5.9.10",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react": "^16.13.1",
    "router": "^1.3.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.3"
  }
}
